case section.type
  when "year"
    div.year.view-section(id="year-#{section.year}")
      div.year-marker
        h1 #{section.year}
      each name in section.names
        include name
  when "interlude"
    div.interlude.view-section(id="#{section.id}")
      each pane in section.panes
        div.pane.narration
          case pane.type
            when "narration-title"
              h3.kicker interlude
              h1.title #{pane.title}
              h2.subtitle #{pane.subtitle}
            when "narration-paragraph"
              p #{pane.paragraph}
